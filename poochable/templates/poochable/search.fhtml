{% load thumbnail %}
<div class="container">
	<br></br>
	<br></br>
	{% if pictures %}
	    <div class="row">
	    {% for picture in pictures %}
	        <a href="/pooch/{{ picture.id }}" data-toggle="lightbox" data-target="#post-picture-lightbox"><div class="span1"><img src="{{ picture.image|thumbnail_url:'avatar' }}" alt=""></div><div class="span4"><h3>{{ picture.dog.name }}</h3></div></a>
	    {% endfor %}
	    </div>
	{% else %}
	    <p>No matching results</p>
	{% endif %}
</div>